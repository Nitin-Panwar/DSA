    "# 1. Find all the paths\n",
    "def all_paths(m, n):\n",
    "    if m==1 or n==1:\n",
    "        return 1\n",
    "    return (all_paths(m-1, n)+all_paths(m, n-1))\n",
    "\n",
    "m=3\n",
    "n=3\n",
    "all_paths(m,n)"
